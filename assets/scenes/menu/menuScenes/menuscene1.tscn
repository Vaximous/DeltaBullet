[sub_resource type="GDScript" id="GDScript_svckf"]
script/source = "extends Node3D
@onready var worldEnv : WorldEnvironment = $worldEnvironment
@onready var playerCharacter : BasePawn = $playerCharacter
func _ready()->void:
	checkPawn()
	$camAnim.play(\"cam\")

	gameManager.updateGraphics(worldEnv)
	if !UserConfig.configs_updated.is_connected(gameManager.updateGraphics.bind(worldEnv)):
		UserConfig.configs_updated.connect(gameManager.updateGraphics.bind(worldEnv))
	else:
		UserConfig.configs_updated.disconnect(gameManager.updateGraphics.bind(worldEnv))
		UserConfig.configs_updated.connect(gameManager.updateGraphics.bind(worldEnv))

func _init()->void:
	gameManager.freeOrphans.connect(free_me_orphan)
func free_me_orphan()->void:
	if not is_inside_tree():
		queue_free()

func checkPawn()->void:
	#playerCharacter.resetBodyShape()
	var lastSaveData : Dictionary
	if gameManager.get_persistent_data().has(\"lastSaveData\"):
		lastSaveData = gameManager.get_persistent_data()[\"lastSaveData\"]

	if lastSaveData:
		playerCharacter.loadPawnFile(lastSaveData.get_or_add(\"pawninfo\",null))
	#playerCharacter.resetBodyShape()
	playerCharacter.checkClothingHider()
"

