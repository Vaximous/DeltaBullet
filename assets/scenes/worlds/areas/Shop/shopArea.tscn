script/source = "extends Node3D
var isInShop = false
@export var shopCam : Camera3D
#@export var shopHud : Control
@onready var header_lbl : Label = $control/panel/label

func _ready():
#	shopHud.hide()
	Dialogic.VAR.set('tsimkShopRadioDestroyed',false)
	shopCam.current = false
	Dialogic.signal_event.connect(_on_dialogic_signal)

func _on_dialogic_signal(argument:String):
	if argument == \"tsimkShop\":
		await Fade.fade_out(0.3, Color(0,0,0,1),\"GradientVertical\",false,true).finished
		Dialogic.end_timeline()
		enableShop()



func _unhandled_input(event):
	if Input.is_action_just_pressed(\"gEscape\"):
		if isInShop:
			disableShop()

func _on_button_pressed() -> void:
	get_tree().paused = true
	await Fade.fade_out(0.3, Color(0,0,0,1),\"GradientVertical\",false,true).finished
	get_tree().paused = false
	get_tree().change_scene_to_file(\"res://assets/scenes/worlds/areas/playerHome/playerHome.tscn\")

func enableShop():
		isInShop = true
		shopCam.current = true
		gameManager.showMouse()
		#shopHud.show()
		gameManager.pauseMenu.canPause = false
		gameManager.activeCamera.hud.hudEnabled = false
		gameManager.activeCamera.followingEntity.hide()
		await Fade.fade_in(0.3, Color(0,0,0,1),\"GradientVertical\",false,true).finished
		gameManager.initShop(load(\"res://assets/resources/customResources/shopData/shops/uncleTsimkShop.tres\"))

func disableShop():
		gameManager.activeCamera.followingEntity.show()
		gameManager.activeCamera.hud.hudEnabled = true
		isInShop = false
		shopCam.current = false
		#shopHud.hide()
		gameManager.pauseMenu.canPause = true
		gameManager.hideMouse()
		gameManager.activeCamera.camera.make_current()
		gameManager.removeShop()


func _on_hitbox_damaged(amount, impulse, vector):
	Dialogic.VAR.set('tsimkShopRadioDestroyed',true)
"

